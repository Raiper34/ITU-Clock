<Window x:Class="hours.Settings"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Setings" Height="300" Width="300">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="1*" />
            <RowDefinition Height="1*" />
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="1*" />
        </Grid.ColumnDefinitions>
        
        <ComboBox x:Name="SkinBox" SelectionChanged="vyber" Grid.Row="0" >
           
        </ComboBox>
        <Image  x:Name="nahlad" Source="MyImage.jpg" Grid.Row="1"/>
    </Grid>
</Window>
